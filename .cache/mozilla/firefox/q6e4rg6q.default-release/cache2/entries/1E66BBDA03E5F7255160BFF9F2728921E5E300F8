function getHashShort(aHash, aNum) {return (aHash.substr(0,aNum)+"..."+aHash.substr(-aNum))}
getFecha=function(aTimeStamp) {
   var d = new Date()
   var n = d.getTimezoneOffset(); //Minutos de diferencia de la Hora Mundial
  
    var u = new Date(aTimeStamp)
    u.setHours(
    u.getHours()-n/60)
   // alert (u.toGMTString())
    var  hora=u.getUTCHours()
    var minuto=u.getUTCMinutes()
    var segundo = u.getUTCSeconds()
    //+n/60
    var AMPM="AM"
    if (hora>12) {
        hora=hora-12
        AMPM="PM"
    }
    return  ('0' + u.getUTCDate()).slice(-2) +
        '/' + ('0' + (u.getUTCMonth()+1)).slice(-2) +
         '/' + (u.getUTCFullYear()) + 
        ' ' + ('0' + hora).slice(-2)+":"+minuto+ ":"+segundo+" "+AMPM
   
} 


