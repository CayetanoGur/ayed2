  function handleFileSelect( evt) {
    console.log("Aquiiii")
    evt.stopPropagation();
    evt.preventDefault();
    console.log(evt)
    console.log(typeof evt.dataTransfer)
    if (typeof evt.dataTransfer=="undefined") {
        var files = evt.target.files; // FileList object.
    } else {
    var files = evt.dataTransfer.files; // FileList object.
    }
    
    var output1 = [];
    output1.push('<div><strong style="font-size: 16px;color: indianred;font-style: normal;">', 'Waiting for Hashing calculate. ',files.length,' files','</strong></div>');
    document.getElementById('list1').innerHTML = '<div>' + output1.join('') + '</div>';
    var output = [];
    // files is a FileList of File objects. List some properties.
    var total=0
    for (var i = 0, f; f = files[i]; i++) {
        
        var reader = new FileReader();

      // Closure to capture the file information.
      reader.onload = (function(theFile) {
        return function(e) {

            // A qrcode with "ourcodeworld.com" as value in format base64 encoded
           
            /**
             * The function decodeImageFromBase64 expects as first parameter a base64 string from a QRCode.
             * As second parameter the callback that expects the data from the QRCode as first parameter.
             */
            function decodeImageFromBase64(data, callback){
                // set callback
                qrcode.callback = callback;
                // Start decoding
                qrcode.decode(data)
            }
            var xshowqr="";
            decodeImageFromBase64(e.target.result,function(decodedInformation){
                    if (decodedInformation=="Failed to load the image") {
                        decodedInformation="NOT_QR"
                        xshowqr="display:none;";
                        
                    }
                    if (decodedInformation=="error decoding QR Code") {
                        decodedInformation="NOT_QR"
                        xshowqr="display:none;";
                    }
                    //alert("mSG="+decodedInformation);

        
                total++;
                //console.log(total)
                if (total==files.length) {
                    output1 = [];
                    //output1.push('<div><strong>','Finish' ,files.length,' files ','</strong></div>');
                    output1.push('.');
                    document.getElementById('list1').innerHTML = '<div>' + output1.join('') + '</div>';
                } else {
                    output1 = [];
                    output1.push('.');
                    document.getElementById('list1').innerHTML = '<div>' + output1.join('') + '</div>';
                }
              txtFileBase64 =     e.target.result;
            
              sha256txt =  sha256(txtFileBase64)
              
              var qrpk = document.getElementById('qrpk').value;
              if (qrpk=="YES" && decodedInformation!=="NOT_QR") {
                    trxId = hex_sha1(decodedInformation);
                } else {
                    trxId   =   hex_sha1(sha256txt)
                }
                txt="<img style='width: 120px;position: relative;float: left;border-width: 1;border-style: solid;border-radius: 5px;top: 11px;margin-bottom: 14px;' src='"+txtFileBase64+"'>"
                //userId = firebase.auth().currentUser.uid;
                
                //firebase.database().ref('transactions/' + userId+"/"+trxId).once('value').then(function(snapshot) {
                        //console.log("*** Detecto "+'transactions/' + userId+"/"+trxId+"*** ")
                        //console.log(snapshot.val())
                        imgflg=""
                        if (getType(escape(theFile.type))!== "fa-file-image-o") {
                            imgflg="display:none"
                        }
                        
                        if (false) { 
                            if (lang=="es") {
                                txtBoton="Ver"
                                msg1=" El documento ha sido detectado"
                            } else {
                                msg1=" This document has been detected"
                                txtBoton="Show"                         
                            }
                            txt="<div style='position: absolute;'><img style='"+imgflg+";width: 120px;position: relative;float: left;border-width: 1;border-style: solid;border-radius: 5px;top: 11px;margin-bottom: 14px;    left: -50px;top: 0px;' src='"+txtFileBase64+"'><span style='float1: left;color: red;font-size: 12px;font-weight: 500;position: relative;    position: relative;left: -39px;top: -5px;'>"+msg1+"</span></div>"
                            linkAction='<span onclick="window.open(\'https://stamping.io/explorer/?trxid='+trxId+'&lang='+lang+'\')" >'
                            
                        } else {
